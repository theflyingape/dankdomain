{
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "node_modules/": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "terminal.integrated.customGlyphs": true,
  "terminal.integrated.gpuAcceleration": "on",
  "terminal.integrated.scrollback": 1000,
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.tabs.hideCondition": "singleGroup",
  "terminal.integrated.unicodeVersion": "11",
  "workbench.colorCustomizations": {
    "terminal.background": "#000000",
    "terminal.foreground": "#C0C0C0",
    "terminal.ansiBlack": "#000000",
    "terminal.ansiRed": "#8B0000",
    "terminal.ansiGreen": "#228B22",
    "terminal.ansiYellow": "#F4A460",
    "terminal.ansiBlue": "#0000CD",
    "terminal.ansiMagenta": "#BA55D3",
    "terminal.ansiCyan": "#008B8B",
    "terminal.ansiWhite": "#C0C0C0",
    "terminal.ansiBrightBlack": "#696969",
    "terminal.ansiBrightRed": "#FF0000",
    "terminal.ansiBrightGreen": "#90EE90",
    "terminal.ansiBrightYellow": "#FFD700",
    "terminal.ansiBrightBlue": "#4169E1",
    "terminal.ansiBrightMagenta": "#EE82EE",
    "terminal.ansiBrightCyan": "#00FFFF",
    "terminal.ansiBrightWhite": "#FFFAFA",
    "terminalCursor.foreground": "#B0E0E6"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "[ans]": {
    "editor.formatOnSave": false,
    "files.insertFinalNewline": false,
    "files.trimTrailingWhitespace": false
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": false,
    "editor.formatOnSaveMode": "file",
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2,
    "editor.quickSuggestions": {
      "strings": true
    }
  }
}
